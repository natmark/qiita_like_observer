{
  "name": "QiitaLikeObserver",
  "version":"0.1.0",
  "manifest_version": 2,
  "description": "Qiitaの記事にいいねを押した時に、IFTTTのMaker Eventを発生させます。",
  "icons": {
   "16": "icons/icon16.png",
   "48": "icons/icon48.png",
   "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://qiita.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "embeded-script.js"
  ],
  "permissions": [
    "http://qiita.com/*"
  ]
 }
